{    
    "name": "rm_csdn_ad",  
    "manifest_version":2,  
    "version": "1.0",    
    "description": "rm_csdn_ad ",    
    "browser_action": {    
       // "default_icon": "icon.png" ,  
       "default_title": "My Task List",  
        "default_popup": "popup.html"  
     },  
     "background": {  
        "page": "background.html"  
      },  
     "content_scripts": [{  
         "matches": [
            "http://blog.csdn.net/*",
            "http://www.nh87.cn/*",
            "http://www.sis001.com/*",
            "https://www.tube8.com/*"
         ],
         "js": ["resource/jquery.js", "test.js"],   
         "run_at": "document_start",  
         "all_frames": true   
    }],  
    "permissions": [     
    "storage","tabs", "http://*/*", "https://*/*"
    ]     
}  